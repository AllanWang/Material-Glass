<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingTranslation">

    <!-- ShowcaseActivity Strings -->
    <string name="app_name">Material Glass</string>
    <string name="theme_dev">Pitched Apps</string>
    <string name="layers_name">Material Glass</string><!--needs to remain consistent even for different flavours-->
    <string name="app_long_name">Material Glass by Pitched Apps</string>
    <string name="theme_description">Pitched Apps is proud to present Material Glass, cm12\'s first and longest lasting theme! Running since Lollipop, and still going with Marshmallow.
        \nMade by 1 themer
        \nTweaked for 1 500+ hours
        \nUsed by 50 000+ users
        \n\nPress the FAB to apply!</string>
    <string name="layers_new">See in app changelog</string>

    <!-- Muzei Stuff -->
    <string name="artsource_name">@string/app_name</string>
    <string name="artsource_desc">Some wallpapers to go with the theme.</string>

    <string name="json_file_url">https://raw.githubusercontent.com/asdfasdfvful/Pitched-Wallpapers/master/Material_Glass/0wallpapers.json</string>

    <!-- Icon Pack designer/owner e-mail -->
    <string name="email_id">pitchedapps@gmail.com</string>

    <!-- Mail subjects for simple e-mail and requests e-mail -->
    <string name="email_subject">Material Glass Support</string>

    <!-- This are the strings for the cards in the main screen -->
    <string name="welcome_title">Let your wallpaper shine through with material darkness!</string>

    <!-- Strings for Wallpapers and Requests -->
    <string name="request_title">Material Glass Secret Icon Request/</string>
    <string name="request_save_location">%1$s/Material_Glass/Requests/</string>
    <string name="walls_save_location">%1$s/Material_Glass/Wallpapers/</string>

    <!-- Launcher Activity Name
    if the activity is:
    com.packagename.appname.Activity
    then write Activity
    -->
    <string name="main_activity_name">HomeActivity</string>

    <!-- TEXTS TO BE USED IN ABOUT SECTION -->

    <string name="dev_name">Allan W</string>
    <string name="dev_photo" formatted="false" tools:ignore="TypographyDashes">https://raw.githubusercontent.com/asdfasdfvful/Pitched-Wallpapers/master/Material_Glass_Storage/pitchedapps.jpg</string>
    <string name="dev_banner" formatted="false" tools:ignore="TypographyDashes">https://raw.githubusercontent.com/asdfasdfvful/Pitched-Wallpapers/master/Material_Glass_Storage/banner.jpg</string>
    <string name="dev_description">First an icon pack creator, then a themer; now also a developer.</string>
    <string name="dev_gplus">https://plus.google.com/+AllanWPitchedApps</string>
    <string name="dev_gplus_community">https://plus.google.com/u/0/communities/101836645831612238387</string>
    <string name="dev_fb"></string>
    <string name="dev_fb_alt"></string>
    <string name="dev_twitter"></string>
    <string name="dev_twitter_alt"></string>
    <string name="dev_playstore">https://play.google.com/store/apps/dev?id=9057916668129524571</string>
    <!--xda--><string name="dev_xda">http://forum.xda-developers.com/google-nexus-5/themes-apps/theme-material-glass-t3005025</string>
    <string name="dev_youtube"></string>

    <string-array name="dev_sites">
        <item>@string/play_store</item>
        <item>@string/join_community</item>
    </string-array>

    <string-array name="dev_links">
        <item>@string/dev_playstore</item>
        <item>@string/dev_gplus_community</item>
    </string-array>

    <!-- This array will only be used if you enable the List style in About section. -->
    <string-array name="dev_sites_in_list">
        <item>@string/play_store</item>
        <item>@string/google_plus</item>
        <item>@string/join_community</item>
    </string-array>

    <!-- PICTURES NAMES TO BE USED AS PREVIEWS FOR LAUNCHERS -->

    <!-- Feature picture that appears above the description -->
    <string name="theme_feature">icons_banner</string>

    <!-- Previews Pictures Names -->
    <string name="theme_preview1">preview1</string>
    <string name="theme_preview2">preview2</string>

    <!-- These things are needed for some launchers (Feel free to edit, though is not needed) -->
    <string name="theme_title">@string/app_name</string>
    <string name="theme_name">@string/theme_title</string>
    <string name="developer_name">@string/iconpack_author</string>
    <string name="developerName">@string/developer_name</string>
    <string name="theme_author">@string/developer_name</string>
    <string name="authorName">@string/developer_name</string>
    <string name="authorLink">@string/dev_xda</string>
    <string name="theme_info">@string/theme_description</string>
    <string name="theme_ad_msg">@string/theme_description</string>

</resources>