<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pitchedapps.material.glass.test" >

    <uses-feature
        android:name="org.cyanogenmod.theme"
        android:required="false" />

    <meta-data
        android:name="org.cyanogenmod.theme.name"
        android:value="@string/app_name" />
    <meta-data
        android:name="org.cyanogenmod.theme.author"
        android:value="@string/theme_author" />
    <meta-data
        android:name="org.cyanogenmod.theme.email"
        android:value="@string/email_id" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
