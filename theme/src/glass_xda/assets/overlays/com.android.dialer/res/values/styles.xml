<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ListViewStyle" parent="@android:style/Widget.Material.ListView">
        <item name="android:overScrollMode">always</item>
    </style>
    <style name="ContactListFilterTheme" parent="@android:style/Theme.Holo">
        <item name="android:listViewStyle">@style/ListViewStyle</item>
    </style>
    <style name="BackgroundOnlyTheme" parent="@android:style/Theme.Holo">
        <item name="android:windowBackground">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowNoDisplay">true</item>
    </style>
    <!--<style name="Theme.InCallScreen" parent="@android:style/Theme.Material">-->
    <!--<item name="android:windowAnimationStyle">@style/InCallAnimationStyle</item>-->
    <!--<item name="android:actionBarStyle">@style/InCallActionBarStyle</item>-->
    <!--</style>-->
    <style name="InCallPopupMenuStyle" parent="@android:style/Theme.Material" />
    <style name="InCallActionBarStyle" parent="@android:style/Widget.Material.ActionBar">
        <item name="android:textColor">@color/text</item>
        <item name="android:background">@color/backgroundd</item>
    </style>
    <!--<style name="DialtactsTheme" parent="@android:style/Theme.Material" />-->
    <style name="DialtactsActionBarOverflow" parent="@android:style/Widget.Material.ActionButton.Overflow" />
    <style name="DialtactsActionBarOverflowWhite" parent="@android:style/Widget.Material.ActionButton.Overflow" />
    <!--<style name="CallDetailActivityTheme1" parent="@android:style/Theme.Holo" />-->
    <style name="DialtactsActionBarTabTextStyle" parent="@android:style/Widget.Material.ActionBar.TabText" />
    <style name="AlertDialogTheme" parent="@android:style/Theme.Material.Dialog" />
    <style name="CallLogSpinnerStyle">
        <item name="android:textAppearance">@android:style/TextAppearance.Material.Small</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAllCaps">true</item>
    </style>


</resources>
