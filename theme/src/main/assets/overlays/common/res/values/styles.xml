    </style>
    <style name="Theme.Material.Light" parent="@android:style/Theme.Material.Light">
        <item name="android:panelColorBackground">@color/background_light</item>
        <item name="android:panelColorForeground">@android:color/black</item>
        <item name="android:colorBackground">@color/background_light</item>
        <item name="android:colorForeground">@android:color/black</item>
        <item name="android:windowBackground">@color/background_light</item>
