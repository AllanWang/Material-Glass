    <style name="Theme.Material" parent="@android:style/Theme.Material">
        <item name="android:panelColorBackground">@color/backgroundx</item>
        <item name="android:panelColorForeground">@color/text</item>
        <item name="android:colorBackground">@color/backgroundx</item>
        <item name="android:colorForeground">@color/text</item>
        <item name="android:windowBackground">@color/backgroundx</item>
        <item name="android:windowShowWallpaper">false</item>
