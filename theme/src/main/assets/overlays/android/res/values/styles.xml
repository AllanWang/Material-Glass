<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/backgroundt</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@color/backgroundt</item>
        <item name="android:colorBackground">@color/backgroundt</item>
        <item name="android:panelColorForeground">@color/text</item>
        <item name="android:panelColorBackground">@color/backgroundt</item>
    </style>
    <style name="Theme.Black" parent="@android:style/Theme.Black">
        <item name="android:colorBackground">@color/black_t</item>
        <item name="android:windowBackground">@color/black_t</item>
    </style>
    <style name="Theme.Holo" parent="@android:style/Theme.Holo">
        <item name="android:panelColorBackground">@color/backgroundt</item>
        <item name="android:windowBackground">@color/backgroundt</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@color/backgroundt</item>
        <item name="android:colorControlActivated">@color/text</item>
    </style>
    <style name="Theme.Holo.Wallpaper" parent="@android:style/Theme.Holo.Wallpaper">
        <item name="android:windowBackground">@color/backgroundt</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@color/backgroundt</item>
    </style>
    <style name="TextAppearance.Holo.DialogWindowTitle" parent="@android:style/TextAppearance.Holo.DialogWindowTitle">
        <item name="android:textColor">@color/text</item>
    </style>
    <style name="Theme.Material" parent="@android:style/Theme.Material">
        <item name="android:panelColorBackground">@color/backgroundt</item>
        <item name="android:windowBackground">@color/backgroundt</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:panelColorForeground">@color/text</item>
        <item name="android:navigationBarColor">@color/black_t</item>
    </style>
    <!--<style name="Theme.Material.Settings" parent="@android:style/Theme.Material">-->
        <!--<item name="android:colorBackground">@color/backgroundt</item>-->
        <!--<item name="android:colorPrimary">@color/backgroundd</item>-->
        <!--<item name="android:colorPrimaryDark">@color/backgroundd</item>-->
        <!--<item name="android:colorAccent">@color/text</item>-->
    <!--</style>-->
    <style name="Theme.DeviceDefault.Light" parent="@android:style/Theme.DeviceDefault">
        <item name="android:windowBackground">@color/backgroundt</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@color/backgroundt</item>
    </style>

    <style name="Theme.DeviceDefault.Light.Dialog" parent="@android:style/Theme.DeviceDefault.Dialog" />
    <style name="Widget.DeviceDefault.Light" parent="@android:style/Widget.DeviceDefault" />
</resources>
