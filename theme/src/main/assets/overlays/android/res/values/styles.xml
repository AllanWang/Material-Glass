<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.Black" parent="@android:style/Theme">
        <item name="android:colorBackground">@color/black_t</item>
        <item name="android:windowBackground">@color/black_t</item>
    </style>
    <style name="Theme.Holo.Wallpaper" parent="@android:style/Theme.Holo">
        <item name="android:windowBackground">@color/backgroundt</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@color/backgroundt</item>
    </style>
    <style name="Theme.Material.Wallpaper" parent="@android:style/Theme.Material">
        <item name="android:windowBackground">@color/backgroundt</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:colorBackgroundCacheHint">@color/backgroundt</item>
    </style>
    <!--<style name="Theme.DeviceDefault.Light" parent="@style/Theme.Material.Wallpaper"/>-->
</resources>